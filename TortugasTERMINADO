        t1.begin_fill()
t1.forward(110)
################################################################################################################################## TORTUGA 2
t2=Turtle()
t2.pensize(10)

t2.penup()
t2.color("magenta")
t2.goto(-400,0)
t2.pendown()
t2.forward(50)
t2.speed(2)

t2.fillcolor("cyan")
t2.begin_fill()
t2.circle(30)
t2.end_fill()

t2.forward(100)
t2.fillcolor("red")
t2.begin_fill()
t2.circle(30)
t2.end_fill()


t2.forward(130)
t2.fillcolor("blue")
t2.begin_fill()
t2.circle(30)
t2.end_fill()

t2.forward(120)
t2.penup()
t2.right(90)
t2.forward(30)
t2.pendown()
t2.left(180)

t2.fillcolor("orange")
t2.begin_fill()
t2.circle(30)
t2.end_fill()

t2.penup()
t2.forward(30)
t2.pendown()
t2.right(90)


t2.forward(120)
t2.penup()
t2.right(90)
t2.forward(30)
t2.pendown()
t2.left(180)

t2.fillcolor("green")
t2.begin_fill()
t2.circle(30)
t2.end_fill()

t2.penup()
t2.forward(30)
t2.pendown()
t2.right(90)


t2.forward(100)
t2.penup()
t2.right(90)
t2.forward(30)
t2.pendown()
t2.left(180)

t2.fillcolor("black")
t2.begin_fill()
t2.circle(30)
t2.end_fill()

t2.penup()
t2.forward(30)
t2.pendown()
t2.right(90)
t2.forward(100)
################################################################################################################################## TORTUGA 3

t3=Turtle()
t3.speed(10)
t3.color("blue")
t3.penup()
t3.goto(-400,-100)
t3.pendown()
def dibujar_cuadrado():
    for _ in range(4):
        t3.forward(50)
        t3.right(90)
def mover_figura():
    for _ in range(10): 
        dibujar_cuadrado()
        t3.penup()
        t3.forward(70)
        t3.pendown()
    turtle.done()
mover_figura()
